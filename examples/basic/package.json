{
	"name": "arkenv-example-basic",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "tsx --env-file .env index.ts",
		"dev": "tsx watch --env-file .env index.ts",
		"dev:example": "tsx watch --env-file .env.example index.ts",
		"build": "tsc",
		"clean": "rimraf dist node_modules"
	},
	"dependencies": {
		"arkenv": "^0.8.0",
		"arktype": "^2.1.29",
		"zod": "4.2.1"
	},
	"devDependencies": {
		"@types/node": "^24.10.4",
		"rimraf": "^6.1.2",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": "24"
	},
	"stackblitz": {
		"startCommand": "npm install && npm run dev:example"
	},
	"packageManager": "npm@11.6.4"
}
